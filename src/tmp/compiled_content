    <script src="custom_form.js" type="text/javascript" charset="utf-8"></script>
    
    <script type="text/javascript" charset="utf-8">
      $("[type=checkbox], [type=radio], select.custom_form, #custom_file_1").custom_form();
      $("select.custom_form_responsive, #custom_file_2, #custom_file_3").custom_form({
        responsive_select:true,
        responsive_file:true
      })
      
    </script>
